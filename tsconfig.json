{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "moduleResolution": "node",
        "jsx": "react",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "paths": {
            "@/*": ["./resources/js/*"]
        },
        "types": [
            "vite/client",
        ],
        "downlevelIteration": true,
        "lib": [
            "dom",
            "es5",
            "scripthost",
            "es2015.collection"
        ]
    }
}
